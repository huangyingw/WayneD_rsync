[remote "WayneD"]
    url = git@github.com:WayneD/rsync.git
    fetch = +refs/heads/*:refs/remotes/WayneD/*
[remote "origin"]
    url = git@github.com:huangyingw/WayneD_rsync.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = WayneD
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/linux/WayneD/rsync
